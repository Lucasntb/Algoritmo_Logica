Algoritmo "semnome"
//
//
// Descrição   : Aqui você descreve o que o programa faz! (função)

Var

   N1, N2, S: Inteiro
   
Inicio

      Escreva("Informe um numero: ")
      Leia(N1)
      Escreva("Informe outro numero: ")
      Leia(N2)
      S <- N1 + N2
      Escreva("A soma entre", N1, " e", N2, " é :", S)

Fimalgoritmo
Algoritmo "ConteAteN"
// Descrição   : Contar até o numero que o usuario.
Var
   c, n :Inteiro

Inicio
   c <- 0
   Escreval("Até que numero contar?:")
   Leia(n)
// Escreval(c) {Exibir zero primeiro}
   Enquanto (c < n) faca
            c <- c + 1
            Escreval(c)
   FimEnquanto
   


Fimalgoritmo
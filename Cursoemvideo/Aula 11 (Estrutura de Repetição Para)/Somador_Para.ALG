Algoritmo "SomadorPara"
// Descrição   : Soma 5 valores inseridos pelo usuario e exibe na tela, usando Estrutura de repetição PARA

Var
   N, C, S: Inteiro
   
Inicio
      S <- 0
      Para C <- 1 ate 5 faça
           Escreval("Digite um valor: ")
           Leia(N)
      S <- S + N
      FimPara
      EscrevaL("A soma dos 5 valores é igual a:", S)


Fimalgoritmo